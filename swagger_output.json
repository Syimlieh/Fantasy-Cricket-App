{
  "swagger": "2.0",
  "info": {
    "title": "Mariyano Task",
    "description": "Application Similar to Dream 11",
    "version": "1.0.0"
  },
  "host": "localhost:3000",
  "basePath": "/",
  "schemes": [
    "http"
  ],
  "paths": {
    "/add-team": {
      "post": {
        "tags": [
          "Team"
        ],
        "summary": "Create a new Team",
        "description": "Create a new Team",
        "parameters": [
          {
            "name": "obj",
            "in": "body",
            "required": true,
            "description": "Team Detail Object",
            "schema": {
              "type": "object",
              "properties": {
                "teamName": {
                  "type": "string",
                  "example": "Winner Team"
                },
                "players": {
                  "type": "array",
                  "example": [
                    "MS Dhoni",
                    "MJ Santner",
                    "RD Gaikwad",
                    "DP Conway",
                    "N Jagadeesan",
                    "Simarjeet Singh",
                    "YBK Jaiswal",
                    "JC Buttler",
                    "SV Samson",
                    "D Padikkal",
                    "R Parag"
                  ],
                  "items": {
                    "type": "string"
                  }
                },
                "captain": {
                  "type": "string",
                  "example": "MS Dhoni"
                },
                "viceCaptain": {
                  "type": "string",
                  "example": "Robin Uthappa"
                }
              },
              "required": [
                "teamName",
                "players",
                "captain",
                "viceCaptain"
              ]
            }
          }
        ],
        "responses": {
          "default": {
            "description": ""
          }
        }
      }
    },
    "/process-result": {
      "post": {
        "tags": [
          "Result"
        ],
        "summary": "Process Result",
        "description": "Process Result",
        "responses": {
          "default": {
            "description": ""
          }
        }
      }
    },
    "/team-result": {
      "post": {
        "description": "",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  }
}